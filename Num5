#include <iostream>
#include <conio.h>
using namespace std;

int main()
{
    int ctr, num;
        num = 0;
        
    for (ctr = 0 ; ctr < 30 ; ctr++)
    
    {
           if (ctr <= 9){
             num = num+1;
            cout << num << ","; }
       
        else
            {
                if (ctr % 2 == 0){
                num = num+2;
                    cout << num << ","; }
            }
        
    }     

    _getch();   
    return 0;
}
