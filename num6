// Experiment 2 - number 6
#include <iostream>
#include <conio.h>
using namespace std;
 
int main()
{
   int ctr, first = 0, second = 1, next;
        
        cout << "Fibonacci Numbers Sequence" << endl;
   for ( ctr = 0 ; ctr < 20 ; ctr++ )
   {
      if ( ctr <= 1 )
         next = ctr;
      else
      {
         next = first + second;
         first = second;
         second = next;
      }
      cout << next << "," ;
   }
   
_getch();
return 0;
}

